<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="menuMainVM" type="com.vlassapplication.app.modules.menumain.data.viewmodel.MenuMainVM"/>
    </data>
    <LinearLayout android:gravity="start"
        android:id="@+id/linearMenumain"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <LinearLayout android:id="@+id/linearColumnsix"
            android:layout_marginTop="@dimen/_20pxv"
            android:layout_height="0dp" android:layout_weight="1.0"
            android:layout_width="match_parent" android:orientation="vertical">
            <TextView android:ellipsize="end" android:gravity="start"
                android:id="@+id/txtSix"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_21pxh"
                android:layout_marginStart="@dimen/_21pxh"
                android:layout_width="match_parent"
                android:singleLine="true"
                android:text="@{menuMainVM.menuMainModel.txtSix}"
                style="@style/txtMontserratbold30" tools:text="@string/lbl11"/>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearContentmoto"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginStart="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_50pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageImage"
                    android:layout_height="@dimen/_110pxh"
                    android:layout_width="@dimen/_170pxh"
                    android:scaleType="fitXY"
                    cornerRadius="@{@dimen/_35pxh}"
                    imageRes="@{@drawable/img_image}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_image"/>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearColumnyamahavirago"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_33pxh"
                    android:layout_marginStart="@dimen/_19pxh"
                    android:layout_marginTop="@dimen/_2pxv"
                    android:layout_weight="0.39"
                    android:layout_width="0dp" android:orientation="vertical">
                    <TextView android:ellipsize="end"
                        android:gravity="center_horizontal"
                        android:id="@+id/txtYamahaVirago"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_1pxh"
                        android:layout_width="match_parent"
                        android:singleLine="true"
                        android:text="@{menuMainVM.menuMainModel.txtYamahaVirago}"
                        style="@style/txtMontserratsemibold14" tools:text="@string/lbl_yamaha_virago"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtXv1100Counter"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_marginStart="@dimen/_1pxh"
                        android:layout_marginTop="@dimen/_5pxv"
                        android:layout_width="match_parent"
                        android:singleLine="true"
                        android:text="@{menuMainVM.menuMainModel.txtXv1100Counter}"
                        style="@style/txtMontserratregular12" tools:text="@string/lbl_xv1100_1986"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txt10633"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_marginStart="@dimen/_1pxh"
                        android:layout_marginTop="@dimen/_4pxv"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@{menuMainVM.menuMainModel.txt10633}"
                        style="@style/txtMontserratregular12" tools:text="@string/lbl_1063_3"/>
                    <View
                        android:background="@drawable/rectangle_bg_red_800_radius_20"
                        android:id="@+id/viewColor"
                        android:layout_height="@dimen/_40pxh"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_marginTop="@dimen/_9pxv" android:layout_width="@dimen/_40pxh"/>
                </LinearLayout>
            </LinearLayout>
            <LinearLayout android:gravity="center_horizontal"
                android:id="@+id/linearKmage"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_38pxv"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStylegray_200cornerRadius">
                <LinearLayout android:id="@+id/linearRowseven"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_20pxh"
                    android:layout_marginStart="@dimen/_20pxh"
                    android:layout_marginTop="@dimen/_21pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtSeven"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.51"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{menuMainVM.menuMainModel.txtSeven}"
                        style="@style/txtMontserratromanbold18" tools:text="@string/lbl13"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txt21500"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginStart="@dimen/_178pxh"
                        android:layout_marginTop="@dimen/_2pxv"
                        android:layout_weight="0.49"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{menuMainVM.menuMainModel.txt21500}"
                        style="@style/txtMontserratbold14" tools:text="@string/lbl_21_500"/>
                </LinearLayout>
                <ImageView android:gravity="center_horizontal"
                    android:id="@+id/imageImageOne"
                    android:layout_height="@dimen/_128pxh"
                    android:layout_marginBottom="@dimen/_52pxv"
                    android:layout_marginEnd="@dimen/_17pxh"
                    android:layout_marginStart="@dimen/_20pxh"
                    android:layout_marginTop="@dimen/_23pxv"
                    android:layout_width="match_parent"
                    android:scaleType="fitXY"
                    imageRes="@{@drawable/img_image1}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_image1"/>
            </LinearLayout>
            <androidx.appcompat.widget.AppCompatButton
                android:ellipsize="end" android:gravity="center"
                android:id="@+id/btnOne"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginStart="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_12pxv"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_16pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_30pxh"
                android:paddingTop="@dimen/_16pxv"
                android:singleLine="true" android:text="@string/lbl3"
                android:textAllCaps="false"
                style="@style/btnSolidRounded_1" tools:text="@string/lbl3"/>
        </LinearLayout>
        <FrameLayout android:gravity="start"
            android:id="@+id/frameBottombar"
            android:layout_height="wrap_content" android:layout_width="match_parent"
            android:layout_marginBottom="@dimen/_10pxv">
            <FrameLayout android:id="@+id/frameStackrectanglefive"
                android:layout_height="@dimen/_74pxv" android:layout_width="@dimen/_340pxh"
                android:layout_gravity="center">
                <View
                    android:background="@drawable/rectangle_bg_gray_800_radius_37"
                    android:id="@+id/viewRectangleFive"
                    android:layout_height="@dimen/_74pxv" android:layout_width="@dimen/_340pxh" />
                <LinearLayout android:id="@+id/linearRowopenendwrench"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_12pxv"
                    android:layout_marginTop="@dimen/_12pxv"
                    android:layout_width="match_parent"
                    android:gravity="center"
                    android:orientation="horizontal"
                    tools:gravity="center">
                    <FrameLayout android:id="@+id/frameSuppliers"
                        android:layout_height="@dimen/_50pxv"
                        android:layout_width="@dimen/_50pxh" style="@style/groupStylegray_800cornerRadius"
                        android:layout_marginRight="@dimen/_30pxh">
                        <ImageView android:id="@+id/imageOpenEndWrench"
                            android:layout_gravity="center"
                            android:layout_height="@dimen/_30pxh"
                            android:layout_marginHorizontal="@dimen/_10pxh"
                            android:layout_marginVertical="@dimen/_10pxv"
                            android:layout_width="@dimen/_30pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_openendwrench"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_openendwrench"/>
                    </FrameLayout>
                    <FrameLayout android:id="@+id/frameMyBambax"
                        android:layout_height="@dimen/_50pxv"
                        android:layout_width="@dimen/_50pxh" style="@style/groupStylegray_800cornerRadius"
                        android:layout_marginRight="@dimen/_30pxh">
                        <ImageView android:id="@+id/imageDirtBike"
                            android:layout_gravity="center"
                            android:layout_height="@dimen/_30pxh"
                            android:layout_marginHorizontal="@dimen/_10pxh"
                            android:layout_marginVertical="@dimen/_10pxv"
                            android:layout_width="@dimen/_30pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_dirtbike"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_dirtbike"/>
                    </FrameLayout>
                    <FrameLayout android:id="@+id/frameMySettings"
                        android:layout_height="@dimen/_50pxv"
                        android:layout_width="@dimen/_50pxh" style="@style/groupStylegray_800cornerRadius">
                        <ImageView android:id="@+id/imageMore"
                            android:layout_gravity="center"
                            android:layout_height="@dimen/_30pxh"
                            android:layout_marginHorizontal="@dimen/_10pxh"
                            android:layout_marginVertical="@dimen/_10pxv"
                            android:layout_width="@dimen/_30pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_more"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_more"/>
                    </FrameLayout>
                </LinearLayout>
            </FrameLayout>
        </FrameLayout>
    </LinearLayout>
</layout>
